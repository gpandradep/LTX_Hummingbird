	file = {Full Text PDF:C\:\\Users\\Gato\\Zotero\\storage\\RFFBUIVC\\Minin et al. - 2016 - Global priorities for national carnivore conservat.pdf:application/pdf;Snapshot:C\:\\Users\\Gato\\Zotero\\storage\\DF7TBEEL\\srep23814.html:text/html}
