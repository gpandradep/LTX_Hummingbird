@article{kluever_spatial_2016,
	title = {Spatial response of coyotes to removal of water availability at anthropogenic water sites},
	volume = {130},
	issn = {0140-1963},
	url = {http://www.sciencedirect.com/science/article/pii/S0140196316300350},
	doi = {10.1016/j.jaridenv.2016.03.009},
	abstract = {Features containing year-round availability of free water (hereafter water sites) and areas affiliated with water sites (i.e., riparian zones) occurring within arid landscapes represent a potential limiting resource for some desert dwelling vertebrates. Little is known about the relationship between water sites and mammalian carnivores. An increase of water sites in portions of the Great Basin Desert in Utah reportedly contributed to an increase in coyote (Canis latrans) abundance. We examined frequency of visitation and spatial affinity of resident coyotes for water sites at the home range scale extent. Visitation to sites with available water averaged 13.0 visitations/season (SD = 13.5) and ranged from zero to 47. We documented no visits to water sites in 16\% (10 of 64) of seasonal home-ranges, {\textless}5 visits within 39\% (25 of 64) of home ranges, and 25\% (28 of 113) of coyote home-ranges did not contain a water site. Water sites associated with riparian vegetation experienced higher visitation than guzzlers (no riparian vegetation present). We found no evidence that removal of water influenced home range size or spatial shifting of home range areas. Water sites, especially guzzlers, do not represent a pivotal resource for the coyote population in our study area.},
	urldate = {2019-06-27},
	journal = {Journal of Arid Environments},
	author = {Kluever, Bryan M. and Gese, Eric M.},
	month = jul,
	year = {2016},
	keywords = {Canid, Desert, Free water, Guzzler, Home-range},
	pages = {68--75},
	file = {ScienceDirect Full Text PDF:C\:\\Users\\Gato\\Zotero\\storage\\UDEDNQKW\\Kluever y Gese - 2016 - Spatial response of coyotes to removal of water av.pdf:application/pdf;ScienceDirect Snapshot:C\:\\Users\\Gato\\Zotero\\storage\\NUZV5WDZ\\S0140196316300350.html:text/html}
}

