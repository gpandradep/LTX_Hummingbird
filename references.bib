@article{davis_ecological_2018,
	title = {Ecological correlates of the spatial co-occurrence of sympatric mammalian carnivores worldwide},
	volume = {21},
	copyright = {© 2018 John Wiley \& Sons Ltd/CNRS},
	issn = {1461-0248},
	url = {https://onlinelibrary.wiley.com/doi/abs/10.1111/ele.13124},
	doi = {10.1111/ele.13124},
	abstract = {The composition of local mammalian carnivore communities has far-reaching effects on terrestrial ecosystems worldwide. To better understand how carnivore communities are structured, we analysed camera trap data for 108 087 trap days across 12 countries spanning five continents. We estimate local probabilities of co-occurrence among 768 species pairs from the order Carnivora and evaluate how shared ecological traits correlate with probabilities of co-occurrence. Within individual study areas, species pairs co-occurred more frequently than expected at random. Co-occurrence probabilities were greatest for species pairs that shared ecological traits including similar body size, temporal activity pattern and diet. However, co-occurrence decreased as compared to other species pairs when the pair included a large-bodied carnivore. Our results suggest that a combination of shared traits and top-down regulation by large carnivores shape local carnivore communities globally.},
	language = {en},
	urldate = {2018-08-01},
	journal = {Ecology Letters},
	author = {Davis, Courtney L. and Rich, Lindsey N. and Farris, Zach J. and Kelly, Marcella J. and Bitetti, Mario S. Di and Blanco, Yamil Di and Albanesi, Sebastian and Farhadinia, Mohammad S. and Gholikhani, Navid and Hamel, Sandra and Harmsen, Bart J. and Wultsch, Claudia and Kane, Mamadou D. and Martins, Quinton and Murphy, Asia J. and Steenweg, Robin and Sunarto, Sunarto and Taktehrani, Atieh and Thapa, Kanchan and Tucker, Jody M. and Whittington, Jesse and Widodo, Febri A. and Yoccoz, Nigel G. and Miller, David A. W.},
	year = {2018},
	note = {tex.ids: davis\_ecological\_2018-1},
	keywords = {interspecific interactions, Camera trap, ecological traits, global assessment, local community structure, spatial co-occurrence},
	pages = {1401--1412},
	file = {Full Text PDF:C\:\\Users\\Gato\\Zotero\\storage\\XMVBEZZY\\Davis et al. - Ecological correlates of the spatial co-occurrence.pdf:application/pdf;Full Text PDF:C\:\\Users\\Gato\\Zotero\\storage\\QDGU7S8U\\Davis et al. - 2018 - Ecological correlates of the spatial co-occurrence.pdf:application/pdf;Snapshot:C\:\\Users\\Gato\\Zotero\\storage\\N3LT49BX\\ele.html:text/html;Snapshot:C\:\\Users\\Gato\\Zotero\\storage\\NX5S2YT8\\ele.html:text/html}
